<?xml version="1.0" encoding="utf-8"?>
<popmenus>
	<popmenu>
		<name>命令行</name>
		<type>cmd</type>
		<printinfo>false</printinfo>
		<implementor>cmd /c start cmd</implementor>
	</popmenu>
	<popmenu>
		<name>Cygwin</name>
		<type>cmd</type>
		<printinfo>false</printinfo>
		<implementor>cmd /c start D:\cygwin\Cygwin-menu.bat "${dir}"</implementor>
	</popmenu>
	<popmenu>
		<name>发布项目到weblogic</name>
		<type>cmd</type>
		<dependency>Project</dependency>
		<printinfo>true</printinfo>
		<implementor>
			<![CDATA[
				cmd /c ant deploy
			]]>
		</implementor>
	</popmenu>
	<popmenu>
		<name>更新项目到weblogic</name>
		<type>cmd</type>
		<dependency>Project</dependency>
		<printinfo>true</printinfo>
		<implementor>
			<![CDATA[
				cmd /c ant updeploy
			]]>
		</implementor>
	</popmenu>
	<popmenu>
		<name>复制jar</name>
		<type>cmd</type>
		<printinfo>true</printinfo>
		<implementor>
			<![CDATA[
				cmd /c ant copy-jars
			]]>
		</implementor>
	</popmenu>
	<popmenu>
		<name>test</name>
		<type>cmd</type>
		<printinfo>true</printinfo>
		<implementor>
			<![CDATA[
				echo test1 ${build_ejb};
				echo test2 ${build_web};
			]]>
		</implementor>
	</popmenu>
</popmenus>