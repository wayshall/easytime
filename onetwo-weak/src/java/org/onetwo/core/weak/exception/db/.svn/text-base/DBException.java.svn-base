package weak.exception.db;

import org.apache.commons.logging.Log;
import org.apache.commons.logging.LogFactory;

import weak.exception.BaseException;

@SuppressWarnings("serial")
public class DBException extends BaseException {

	protected final Log log = LogFactory.getLog(DBException.class);
	private static final String DefaultMsg = "访问数据发生错误！";
	
	public DBException(){
		super(DefaultMsg);
	}
	
	public DBException(String msg){
		super(msg);
	}
	
	public DBException(Throwable cause){
		super(DefaultMsg, cause, DefaultIsPrintStack);
	}
	
	public DBException(String msg, Throwable cause){
		super(msg, cause);
	}
}
