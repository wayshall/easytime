package weak.exception.db;

import org.apache.commons.logging.Log;
import org.apache.commons.logging.LogFactory;

import weak.exception.BaseException;

@SuppressWarnings("serial")
public class QueryException extends BaseException {

	protected final Log log = LogFactory.getLog(QueryException.class);
	private static final String DefaultMsg = "解释为sql语句时发生错误！";
	
	public QueryException(){
		super(DefaultMsg);
	}
	
	public QueryException(String msg){
		super(msg);
	}
	
	public QueryException(Throwable cause){
		super(DefaultMsg, cause, DefaultIsPrintStack);
	}
	
	public QueryException(String msg, Throwable cause){
		super(msg, cause);
	}
}
